{
	"doca_general_flags": {
		// -l - Set the (numeric) log level for the program <10=DISABLE, 20=CRITICAL, 30=ERROR, 40=WARNING, 50=INFO, 60=DEBUG, 70=TRACE>
		"log-level": 60,
		// -g - grpc address is mandatory flag. may add ports to address.
		"grpc-address": "0.0.0.0",
	},
	"doca_program_flags": {
		// Set firewall running mode {static, interactive}
		"mode": "static",
		// Path to the JSON file with 5-tuple rules when running with static mode
		"firewall-rules": "firewall_rules.json",
	}
}
